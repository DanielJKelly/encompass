{
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "env": {
        "node": true,
        "browser": true,
        "mocha": true,
        "amd": true,
        "es6": true
    },
    "rules": {
        "no-console":"off",
        "no-unused-vars": ["warn", { "args": "none" }]
    },
    "globals":{
        "Encompass": true,
        "DS": true,
        "Ember": true,
        "moment": true,
        "guiders": true,
        "$": true,
        "_": true,
        "SelectionHighlighting": true



    }
}