{{#each options.inputs as |input|}}
  <ol>
    <li>
      <input
        type="radio"
        checked={{is-equal selectedValue input.value}}
        required={{options.isRequired}}
        name={{options.groupName}}
        value={{input.value}}
        onclick={{action "setValue"}}
        >
      <label>{{input.label}}</label>
    </li>
  </ol>
{{/each}}